{
	"name": "laravel/laravel",
	"type": "project",
	"description": "The Laravel Framework.",
	"keywords": [
		"framework",
		"laravel"
	],
	"license": "MIT",
	"require": {
		"php": "^7.3|^8.0",
		"chillerlan/php-qrcode": "^4.3",
		"fideloper/proxy": "^4.4",
		"fruitcake/laravel-cors": "^2.0",
		"guzzlehttp/guzzle": "^7.0.1",
		"jeroennoten/laravel-adminlte": "^3.6",
		"laravel/framework": "^8.40",
		"laravel/sanctum": "^2.11",
		"laravel/tinker": "^2.5",
		"simplesoftwareio/simple-qrcode": "^4.2"
	},
	"require-dev": {
		"facade/ignition": "^2.5",
		"fakerphp/faker": "^1.9.1",
		"laravel/breeze": "^1.2",
		"laravel/sail": "^1.0.1",
		"mockery/mockery": "^1.4.2",
		"nunomaduro/collision": "^5.0",
		"phpunit/phpunit": "^9.3.3"
	},
	"autoload": {
		"psr-4": {
			"App\\": "app/",
			"Database\\Factories\\": "database/factories/",
			"Database\\Seeders\\": "database/seeders/",
			"App\\Admin\\Controllers\\": "src/Admin/Application/Controllers/",
			"App\\Admin\\Middleware\\": "src/Admin/Application/Middleware/",
			"App\\Admin\\Providers\\": "src/Admin/Application/Providers/",
			"App\\Admin\\Repositories\\": "src/Admin/Domain/Repositories/",
			"App\\Admin\\Requests\\": "src/Admin/Domain/Requests/",
			"App\\Admin\\Interfaces\\": "src/Admin/Infrastructure/Interfaces/",
			"App\\Admin\\Services\\": "src/Admin/Infrastructure/Services/",
			"App\\User\\Controllers\\": "src/User/Application/Controllers/",
			"App\\User\\Middleware\\": "src/User/Application/Middleware/",
			"App\\User\\Providers\\": "src/User/Application/Providers/",
			"App\\User\\Repositories\\": "src/User/Domain/Repositories/",
			"App\\User\\Requests\\": "src/User/Domain/Requests/",
			"App\\User\\Interfaces\\": "src/User/Infrastructure/Interfaces/",
			"App\\User\\Services\\": "src/User/Infrastructure/Services/",
			"App\\Common\\Controllers\\": "src/Common/Application/Controllers/",
			"App\\Common\\Middleware\\": "src/Common/Application/Middleware/",
			"App\\Common\\Providers\\": "src/Common/Application/Providers/",
			"App\\Common\\Models\\": "src/Common/Domain/Models/",
			"App\\Common\\Repositories\\": "src/Common/Domain/Repositories/",
			"App\\Common\\Requests\\": "src/Common/Domain/Requests/",
			"App\\Common\\Interfaces\\": [
				"src/Common/Infrastructure/Interfaces/",
				"src/Common/Domain/Interfaces/"
			],
			"App\\Common\\Services\\": "src/Common/Infrastructure/Services/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"scripts": {
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover --ansi"
		],
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate --ansi"
		]
	},
	"extra": {
		"laravel": {
			"dont-discover": []
		}
	},
	"config": {
		"optimize-autoloader": true,
		"preferred-install": "dist",
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
